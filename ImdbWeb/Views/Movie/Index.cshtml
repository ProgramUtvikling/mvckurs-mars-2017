@model IEnumerable<Movie>

<h1>@(Model.GetTitle() ?? "Filmer")</h1>

<ul>
    @foreach (var movie in Model)
    {
        <li>
            @*@Html.ActionLink(movie.Title, "Details", new { id = movie.MovieId })*@
            <a asp-action="Details" asp-controller="Movie" asp-route-id="@movie.MovieId">@movie.Title</a>
        </li>
    }
</ul>